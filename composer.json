{
    "name": "backendtea/php-optimizer",
    "type": "library",
    "description": "Optimize PHP code for phars",
    "license": "MIT",
    "authors": [
        {
            "name": "Gert de Pagter",
            "email": "gertp93@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "nikic/php-parser": "^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PhpOptimizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpOptimizer\\Test\\": "tests/"
        }
    }
}
